{
  "name": "backport-action",
  "version": "3.5.0-SNAPSHOT",
  "private": false,
  "description": "GitHub action to automatically backport pull requests",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"**.ts\"",
    "format-check": "prettier --check \"**.ts\"",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --silent",
    "test-verbose": "jest",
    "all": "npm run format && npm run build && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/korthout/backport-action.git"
  },
  "keywords": [
    "actions",
    "backport"
  ],
  "author": "korthout",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0 <25"
  },
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.1",
    "@types/dedent": "0.7.2",
    "dedent": "1.7.0",
    "execa": "7.2.0"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@vercel/ncc": "0.38.4",
    "jest": "30.2.0",
    "prettier": "3.6.2",
    "ts-jest": "29.4.4",
    "typescript": "5.9.3"
  }
}
