{
  "name": "backport-action",
  "version": "0.0.3",
  "private": true,
  "description": "GitHub action to automatically backport pull requests",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"**.ts\"",
    "format-check": "prettier --check \"**.ts\"",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --silent",
    "test-verbose": "jest",
    "all": "npm run format && npm run build && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeebe-io/backport-action.git"
  },
  "keywords": [
    "actions",
    "backport"
  ],
  "author": "zeebe-io",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^5.0.0",
    "@types/dedent": "^0.7.0",
    "dedent": "^0.7.0"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^3.75.2",
    "@types/jest": "^26.0.10",
    "@types/node": "^15.6.1",
    "@vercel/ncc": "^0.28.6",
    "exec-sh": "^0.4.0",
    "jest": "^27.0.3",
    "jest-circus": "^27.0.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.0",
    "ts-jest": "^27.0.1",
    "typescript": "^4.2.4"
  }
}
