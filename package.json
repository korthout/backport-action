{
  "name": "backport-action",
  "version": "0.0.3",
  "private": true,
  "description": "GitHub action to automatically backport pull requests",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/",
    "format-check": "prettier --check src/",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --silent",
    "all": "npm run format && npm run build && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeebe-io/backport-action.git"
  },
  "keywords": [
    "actions",
    "backport"
  ],
  "author": "zeebe-io",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "~1.2.6",
    "@actions/exec": "~1.0.4",
    "@actions/github": "~4.0.0",
    "@octokit/webhooks": "~7.15.0",
    "@types/dedent": "~0.7.0",
    "dedent": "~0.7.0"
  },
  "devDependencies": {
    "@types/jest": "~26.0.10",
    "@types/node": "~14.10.0",
    "@vercel/ncc": "~0.23.0",
    "jest": "^26.0.0",
    "jest-circus": "~26.4.2",
    "js-yaml": "~3.14.0",
    "prettier": "2.1.1",
    "ts-jest": "^26.0.0",
    "typescript": "~4.0.2"
  }
}
