{
  "name": "backport-action",
  "version": "0.0.1",
  "private": true,
  "description": "GitHub action to automatically backport pull requests",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeebe-io/backport-action.git"
  },
  "keywords": [
    "actions",
    "backport"
  ],
  "author": "zeebe-io",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@octokit/webhooks": "^7.15.0",
    "@types/dedent": "^0.7.0",
    "dedent": "^0.7.0"
  },
  "devDependencies": {
    "@types/node": "^14.10.0",
    "@vercel/ncc": "^0.23.0",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "typescript": "^4.0.2"
  }
}
